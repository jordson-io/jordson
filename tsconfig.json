{
  "exclude": [
    "public",
    "node_modules"
  ],
  "compilerOptions": {
    "lib": [ "DOM", "ES5", "ES6","ES2017", "ES2018", "ES2019", "ES2020"],
    "target": "ES6",
    "module": "CommonJS",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true
  }
}
  "devDependencies": {
    "argon2": "^0.27.0",
    "mongodb": "^3.6.2",
    "nodemailer": "^6.4.11",
    "sass": "^1.26.11",
    "terser": "^4.8.0"
  },
  "dependencies": {}
}
